{
  "name": "Clearquest Passport",
  "description": "This is toolkit of auto input password and login web IBM ClearQuest system, Thanks for MazeNL77 shared his icon",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "tabs",
	"http://*/*",
	"https://*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Setting",
	"default_popup": "setting.html",
	"default_icon":{
	"19":"icon/19.png",
	"38":"icon/38.png"
	}
  },
  "icons":{
  "16":"icon/16.png",
  "32":"icon/32.png",
  "48":"icon/48.png",
  "128":"icon/128.png"
  },
  "manifest_version": 2
}
